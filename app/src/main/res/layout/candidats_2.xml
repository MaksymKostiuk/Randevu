        android:text="Люблю путешествия ,фильмы , прогулки
с друзьями и вкусную еду.
Ищу новые знакомства по интересам, так
что ,пиши;)"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="510dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:cardCornerRadius="30dp"
        app:cardElevation="0dp"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="85dp"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/imageView25"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/profile"
            tools:ignore="MissingConstraints" />
    </androidx.cardview.widget.CardView>

    <ImageView
        android:layout_width="94dp"
        android:layout_height="64dp"
        android:src="@drawable/ic_blue_heart"
        app:layout_constraintEnd_toStartOf="@+id/imageView26"
        app:layout_constraintStart_toEndOf="@+id/imageView27"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="614dp" />

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="64dp"
        android:layout_height="73dp"
        android:layout_marginStart="68dp"
        android:src="@drawable/ic_blue_mes"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="605dp" />

    <ImageView
        android:id="@+id/imageView26"
        android:layout_width="60dp"
        android:layout_height="77dp"
        android:layout_marginEnd="72dp"
        android:src="@drawable/ic_blue_small"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="601dp" />


</androidx.constraintlayout.widget.ConstraintLayout>